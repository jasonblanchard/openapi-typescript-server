{
  "name": "openapi-typescript-server-monorepo",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen:petstore": "openapi-typescript ./examples/petstore/spec.yaml --output ./examples/petstore/gen/schema.d.ts",
    "gen:simple": "openapi-typescript ./examples/simple/spec.yaml --output ./examples/simple/gen/schema.d.ts && openapi-typescript-server build -s ./examples/simple/spec.yaml -o ./examples/simple/gen -t ./schema.d.ts",
    "gen:examples": "npm run gen:petstore && npm run gen:simple"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "workspaces": [
    "packages/*",
    "openapi-typesript-server-express"
  ],
  "devDependencies": {
    "@types/express": "^5.0.0",
    "openapi-typescript": "^7.6.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "express": "^4.21.2",
    "openapi-typescript-server": "^0.0.1"
  }
}
