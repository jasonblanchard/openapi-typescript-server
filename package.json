{
  "name": "openapi-typescript-server-monorepo",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "gen:petstore": "openapi-typescript ./examples/petstore/spec.yaml --output ./examples/petstore/gen/schema.d.ts && openapi-typescript-server build -s ./examples/petstore/spec.yaml -t ./schema.d.ts -o ./examples/petstore/gen ",
    "gen:simple": "openapi-typescript ./examples/simple/spec.yaml --output ./examples/simple/gen/schema.d.ts && openapi-typescript-server build -s ./examples/simple/spec.yaml -t ./schema.d.ts -o ./examples/simple/gen",
    "gen": "npm run gen:petstore && npm run gen:simple",
    "build:all": "npm run build --workspaces --if-present",
    "pack": "npm run build:all && npm pack --workspaces --pack-destination /tmp"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "workspaces": [
    "packages/*",
    "openapi-typesript-server-express"
  ],
  "devDependencies": {
    "@types/express": "^5.0.0",
    "openapi-typescript": "^7.6.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "express": "^4.21.2",
    "express-openapi-validator": "^5.4.6",
    "openapi-typescript-server": "^0.0.1",
    "openapi-typescript-server-express": "^0.0.1"
  }
}
