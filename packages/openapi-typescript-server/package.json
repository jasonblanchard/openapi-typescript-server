{
  "name": "openapi-typescript-server",
  "version": "0.0.15-2",
  "description": "Codegenerator for OpenAPI server stubs in TypeScript",
  "author": "jasonblanchard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonblanchard/openapi-typescript-server.git",
    "directory": "packages/openapi-typescript-server"
  },
  "keywords": [
    "swagger",
    "typescript",
    "ts",
    "dts",
    "openapi",
    "codegen",
    "generation",
    "openapi 3",
    "node",
    "express",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app",
    "api"
  ],
  "type": "module",
  "bin": "./bin/cli/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "node --test",
    "build": "tsup src/cli/index.ts --format cjs --out-dir bin/cli --clean"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "js-yaml": "^4.1.1",
    "ts-morph": "^27.0.2",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "openapi-typescript-server-runtime": "workspace:*"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "openapi-typescript-server-runtime": "workspace:*",
    "tsup": "^8.5.1"
  }
}
